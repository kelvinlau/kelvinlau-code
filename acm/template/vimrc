syntax on
filetype on
set nocp nu ru bs=2 bg=dark ai si cin fo+=ro ts=2 sw=2 sts=2 et is nohls mouse=a aw ar wmnu nowrap noswf nobackup
" set mp=g++\ %\ -o\ %:r
" map <F9> :make<CR>
" map <F5> :!./%:r <CR>
" map <C-F5> :!./%:r < %:r.in <CR>
map <F3> :s!^!//!<CR>
map <F4> :s!^//!!<CR>
" map <F2> ggVG"+y
" map <F2> ggVG:!xclip -sel clip<CR>u
map <C-h> gT
map <C-l> gt
" colorscheme desert
